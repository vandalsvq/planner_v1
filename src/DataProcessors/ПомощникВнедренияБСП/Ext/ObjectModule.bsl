Функция Подключаемый_ПечатьПроверитьСоставОбъектов()
	
	ТаблицаПроверки = НоваяТаблицаПроверки();
	
	// -- Проверим наличие процедуры печати табличных документов в модуле менеджеров объектов
	КаталогСМодулями =  КаталогВыгрузкиМодулей;
	МассивФайлов = НайтиФайлы(КаталогСМодулями, "*.Команда.*.txt");
	
	МодулиМенеджеров = СформироватьИменаМодулей(,,Истина);
	
	ПечатаемыеОбъекты  = Новый Массив;
	
	Для Каждого ОписаниеФайла Из МассивФайлов Цикл
		Подстроки = РазложитьСтрокуВМассивПодстрок(ОписаниеФайла.ИмяБезРасширения, ".");
		ПолноеИмяОМСКомандой = Подстроки[0]+"."+Подстроки[1]; 
		ОМ = Метаданные.НайтиПоПолномуИмени(ПолноеИмяОМСКомандой);// этот объект метаданных может содержать команду печати другого объекта
		Если ОМ = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		Для Каждого Команда Из ОМ.Команды Цикл
			Если Метаданные.ГруппыКоманд.Печать = Команда.Группа.Получить() Тогда
				СписокТиповКоманды = Команда.ТипПараметраКоманды.Типы();
				Если СписокТиповКоманды.Количество() = 0 Тогда
					Продолжить;
				КонецЕсли;
				ТипОбъекта = СписокТиповКоманды[0];
				ПолноеИмяОбъектаПечати = Метаданные.НайтиПоТипу(ТипОбъекта).ПолноеИмя();
				Если ПечатаемыеОбъекты.Найти(ПолноеИмяОбъектаПечати) <> Неопределено Тогда
					Продолжить;
				КонецЕсли;
				
				Индекс = МодулиМенеджеров.Найти(ПолноеИмяОбъектаПечати+".МодульМенеджера");
				ИмяМодуляМенеджераОбъектаПечати = ?(Индекс = Неопределено, "", ПолноеИмяОбъектаПечати+".МодульМенеджера");
				Индекс = МодулиМенеджеров.Найти(ПолноеИмяОМСКомандой+".МодульМенеджера");
				ИмяМодуляМенеджераОбъектаСКомандой = ?(Индекс = Неопределено, "", ПолноеИмяОМСКомандой+".МодульМенеджера");
				
				Если	НЕ
						(ПроверитьВхождениеСтрокиВМодуль(ИмяМодуляМенеджераОбъектаПечати, "Печать", Истина, "Печать(МассивОбъектов, ПараметрыПечати, КоллекцияПечатныхФорм, ОбъектыПечати, ПараметрыВывода) Экспорт")
					ИЛИ ПроверитьВхождениеСтрокиВМодуль(ИмяМодуляМенеджераОбъектаСКомандой, "Печать", Истина, "Печать(МассивОбъектов, ПараметрыПечати, КоллекцияПечатныхФорм, ОбъектыПечати, ПараметрыВывода) Экспорт")
					ИЛИ ПроверитьВхождениеСтрокиВМодуль(ИмяМодуляМенеджераОбъектаПечати, "ПолучитьДанныеПечати", Ложь, "ПолучитьДанныеПечати(Знач МассивДокументов, Знач МассивИменМакетов) Экспорт")
					ИЛИ ПроверитьВхождениеСтрокиВМодуль(ИмяМодуляМенеджераОбъектаСКомандой, "ПолучитьДанныеПечати", Ложь, "ПолучитьДанныеПечати(Знач МассивДокументов, Знач МассивИменМакетов) Экспорт") ) Тогда
					Продолжить;
				КонецЕсли;
				
				Если Команда.РежимИспользованияПараметра = РежимИспользованияПараметраКоманды.Одиночный Тогда
					ДобавитьОписаниеПроизвольнойОшибки(ТаблицаПроверки,
						ПодставитьПараметрыВСтроку(Нстр("ru = 'Команда печати %1 не отмечена для множественного использования'"), Команда.ПолноеИмя()));
				КонецЕсли;
				
				ПечатаемыеОбъекты.Добавить(ПолноеИмяОбъектаПечати);
				
			КонецЕсли;
		КонецЦикла;
	КонецЦикла;
	
	ПроверенныеДанные = "";
	
	Для Каждого ИмяПечатаемогоОбъекта Из ПечатаемыеОбъекты Цикл
		ПроверенныеДанные = ПроверенныеДанные + ИмяПечатаемогоОбъекта + ",";
	КонецЦикла;
	
	Если НЕ ПустаяСтрока(ПроверенныеДанные) Тогда
		ПроверенныеДанные = Лев(ПроверенныеДанные, СтрДлина(ПроверенныеДанные)-1);
	КонецЕсли;
	
	РезультатПроверки = ТаблицаПроверки.Добавить();
	РезультатПроверки.РеквизитПроверки = Нстр("ru = 'Проверка команд печатных форм'");
	РезультатПроверки.ПроверенныеДанные = ПроверенныеДанные;
	
	Возврат ТаблицаПроверки;
	
КонецФункции

